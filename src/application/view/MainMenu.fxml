<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane minHeight="550.0" minWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.MainMenuController">
   <center>
      <Pane prefHeight="500.0" prefWidth="500.0" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="74.0" layoutY="193.0" mnemonicParsing="false" onAction="#soloGameButtonPressed" prefHeight="57.0" prefWidth="353.0" text="Solo Game">
               <font>
                  <Font name="Bell MT" size="20.0" />
               </font>
            </Button>
            <Button layoutX="73.0" layoutY="269.0" mnemonicParsing="false" onAction="#leaderboardButtonPressed" prefHeight="55.0" prefWidth="355.0" text="Leaderboard">
               <font>
                  <Font name="Bell MT" size="20.0" />
               </font>
            </Button>
            <Button layoutX="73.0" layoutY="344.0" mnemonicParsing="false" onAction="#exitButtonPressed" prefHeight="52.0" prefWidth="355.0" text="Exit">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
            <Label layoutX="175.0" layoutY="110.0" text="Main Menu">
               <font>
                  <Font name="Bell MT Bold" size="30.0" />
               </font>
            </Label>
            <Label layoutX="190.0" layoutY="10.0" text="BlockD" textFill="#5d0d64e8">
               <font>
                  <Font name="Gill Sans Ultra Bold Condensed" size="40.0" />
               </font>
               <effect>
                  <Reflection />
               </effect>
            </Label>
         </children>
      </Pane>
   </center>
</BorderPane>
